{
  "name": "cudnt-enterprise",
  "version": "1.0.0",
  "description": "CUDNT Enterprise Features - Complete Business Platform",
  "scripts": {
    "deploy:production": "bash scripts/deploy.sh",
    "terraform:init": "cd terraform && terraform init",
    "terraform:plan": "cd terraform && terraform plan",
    "terraform:apply": "cd terraform && terraform apply",
    "monitoring:setup": "kubectl apply -f monitoring/",
    "health:check": "curl -f http://localhost:3000/api/health",
    "health:deep": "curl -f http://localhost:3000/api/health/deep",
    "load:test": "artillery quick --count 100 --num 10 http://localhost:3000/api/health",
    "analytics:report": "curl -f http://localhost:3000/api/analytics/business",
    "stripe:webhook": "stripe listen --forward-to localhost:3000/api/payments/webhook",
    "crypto:monitor": "node scripts/crypto-monitor.js",
    "sales:lead-score": "node scripts/lead-scoring.js",
    "email:campaign": "node scripts/email-campaign.js",
    "demo:schedule": "node scripts/demo-scheduler.js",
    "roi:calculate": "node scripts/roi-calculator.js",
    "backup:create": "bash scripts/backup.sh",
    "backup:restore": "bash scripts/restore.sh",
    "security:audit": "npm audit && trivy image cudnt-backend:latest",
    "performance:benchmark": "node scripts/benchmark.js",
    "prime aligned compute:validate": "node scripts/prime aligned compute-validation.js"
  },
  "dependencies": {
    "stripe": "^12.0.0",
    "coinbase-commerce": "^1.1.0",
    "sendgrid": "^7.7.0",
    "twilio": "^4.14.0",
    "chart.js": "^4.4.0",
    "moment": "^2.29.4",
    "uuid": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "redis": "^4.6.0",
    "mongoose": "^7.5.0",
    "prom-client": "^14.2.0",
    "winston": "^3.10.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^3.0.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/bcryptjs": "^2.4.3",
    "typescript": "^5.2.0",
    "jest": "^29.6.0",
    "supertest": "^6.3.3",
    "artillery": "^2.0.5",
    "trivy": "^0.45.0",
    "terraform": "^1.6.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "cudnt",
    "enterprise",
    "prime aligned compute",
    "mathematics",
    "optimization",
    "business",
    "saas",
    "payments",
    "analytics"
  ],
  "author": "CUDNT Enterprise Team",
  "license": "PROPRIETARY",
  "repository": {
    "type": "git",
    "url": "https://github.com/cudnt/cudnt-enterprise.git"
  },
  "homepage": "https://cudnt.com/enterprise",
  "bugs": {
    "url": "https://github.com/cudnt/cudnt-enterprise/issues"
  },
  "funding": {
    "type": "corporate",
    "url": "https://cudnt.com/funding"
  }
}
