[run]
command = "python main.py --web"

[interpreter]
command = "python"

[env]
PYTHONPATH = "."

[ports]
"8080" = { public = true }

[files]
"README.md" = true
"REPLIT_README.md" = true
"SQUASHPLOT_TECHNICAL_WHITEPAPER.md" = true

[git]
init.ignore = [
  "*.pyc",
  "__pycache__/",
  ".env",
  "node_modules/",
  "plots/",
  "*.log"
]