{
  "scripts": {
    "ionic:serve": "ng serve",
    "server": "node server/server.js",
    "server:dev": "NODE_ENV=development node server/server.js",
    "server:prod": "NODE_ENV=production node server/server.js",
    "server:cluster": "NODE_ENV=production ENABLE_CLUSTERING=true node server/server.js",
    "build:serve": "npm run build:prod && npm run server:prod",
    "build:web": "ng build --configuration=production-web",
    "build:android": "ng build --configuration=production-android",
    "build:ios": "ng build --configuration=production-ios",
    "docker:build": "docker build -t chaios-frontend . -f docker/Dockerfile.client",
    "docker:run": "docker run -p 4200:4200 chaios-frontend"
  },
  "private": true,
  "dependencies": {}
}
